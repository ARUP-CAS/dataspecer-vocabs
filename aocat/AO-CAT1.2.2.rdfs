        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">This property indicates the latitude value of the coordinates of a Spatial Region</rdfs:comment>
        <rdfs:label xml:lang="en">has latitude</rdfs:label>
    </rdf:Property>

	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_longitude -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_longitude">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_Point"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">This property indicates the minimum longitude value of the coordinates of a Spatial Region</rdfs:comment>
        <rdfs:label xml:lang="en">has longitude</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_min_lat -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_min_lat">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_BBox"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">This property indicates the minimum latitude value of the bounding box area defining a Spatial Region</rdfs:comment>
        <rdfs:label xml:lang="en">has bounding box min lat</rdfs:label>
    </rdf:Property> 
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_min_lon -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_min_lon">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_BBox"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">This property indicates the minimum longitude value of the bounding box area defining a Spatial Region</rdfs:comment>
        <rdfs:label xml:lang="en">has bounding box min lon</rdfs:label>
    </rdf:Property>

	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_max_lat -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_max_lat">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_BBox"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">This property indicates the maximum latitude value of the bounding box area defining a Spatial Region</rdfs:comment>
        <rdfs:label xml:lang="en">has bounding box max lat</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_max_lon -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_bounding_box_max_lon">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_BBox"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">This property indicates the maximum longitude value of the bounding box area defining a Spatial Region</rdfs:comment>
        <rdfs:label xml:lang="en">has bounding box max lon</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_place_IRI -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_place_IRI">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_StdName"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">This property associates a spatial region with a URI/IRI or other unique identifier from a standard gazetteer (e.g. Geonames for modern places, Pleiades for ancient places) used to refer a place or a spatialRegion</rdfs:comment>
        <rdfs:label xml:lang="en">has place IRI</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_place_IRI_of -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_place_IRI_of">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
		<rdfs:range rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_StdName"/>
        <rdfs:comment xml:lang="en">inverse of has_place_IRI</rdfs:comment>
        <rdfs:label xml:lang="en">is place IRI of</rdfs:label>
    </rdf:Property>
    
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_polygonal_representation -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_polygonal_representation">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_Polygon"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral"/>
        <rdfs:comment xml:lang="en">This property associates a spatial region with an XML representation of the polygon, typically handled by a GIS</rdfs:comment>
        <rdfs:label xml:lang="en">has polygonal representation</rdfs:label>
    </rdf:Property>  
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_institution -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_institution">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This property specifies the institution where an agent belongs</rdfs:comment>
        <rdfs:label xml:lang="en">has institution</rdfs:label>
    </rdf:Property>   
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_mirror_of -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_mirror_of">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Digital_Media"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">This property associates a digital media with the image it is a mirror of. The latter media is supposed to be on some provider’s site, identified by an IRI.</rdfs:comment>
        <rdfs:label xml:lang="en">is mirror of</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_country -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_country">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">This property associates a spatial region with a country. The range of the property is a wikidata URL (e.g. http://www.wikidata.org/entity/Q16) and the rdfs:label of the URL is the name of the country</rdfs:comment>
        <rdfs:label xml:lang="en">has country</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_data_type -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_data_type">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Individual_Data_Resource"/>
        <rdfs:range rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Concept"/>
        <rdfs:subPropertyOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_type"/>
        <rdfs:comment xml:lang="en">This property associates an ARIADNE data resource with a term from a controlled vocabulary that specifies the data type of the resource. 
		The data type refers to the categorisation of data based on its form, such as text, image, audio, video, etc. 
		The terms used to describe the data type are defined in a controlled vocabulary of resource types specific to ARIADNE (e.g, 3D, GIS, XML).</rdfs:comment>
        <rdfs:label xml:lang="en">has data type</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_data_type_of -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_data_type_of">
        <rdfs:subPropertyOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_type_of"/>
		<rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Concept"/>
        <rdfs:range rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Individual_Data_Resource"/>
        <rdfs:comment xml:lang="en">This is the inverse of property has_data_type, thus it connects an AO_Concept to an AO_Data_Resource that is of that type.</rdfs:comment>
        <rdfs:label xml:lang="en">is type of</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_data_format -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_data_format">
        <rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Individual_Data_Resource"/>
        <rdfs:range rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Concept"/>
        <rdfs:subPropertyOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/has_type"/>
        <rdfs:comment xml:lang="en">This property associates an ARIADNE data resource with a term from a controlled vocabulary that specifies the data format of the resource. 
		The data format refers to the specific file format or encoding format used to store or represent the data, such as CSV, JSON, XML, JPEG, MP3, etc. 
		The term will be selected from the MIME type, a standard vocabulary of formats providing a shared way of identifying the format of a resource on the internet. 
		For example, the MIME type for a CSV file is “text/csv”, and the MIME type for a JPEG file is “image/jpeg”. 
		The MIME type can be also used to help determine how to process or display the data resource.</rdfs:comment>
        <rdfs:label xml:lang="en">has data type</rdfs:label>
    </rdf:Property>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_data_format_of -->

    <rdf:Property rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_data_format_of">
        <rdfs:subPropertyOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/is_type_of"/>
		<rdfs:domain rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Concept"/>
        <rdfs:range rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Individual_Data_Resource"/>
        <rdfs:comment xml:lang="en">This is the inverse of property has_data_format, thus it connects an AO_Concept to an AO_Data_Resource that is of that format.</rdfs:comment>
        <rdfs:label xml:lang="en">is type of</rdfs:label>
    </rdf:Property>
