    <!-- http://www.cidoc-crm.org/cidoc-crm/E1_CRM_Entity -->

    <rdfs:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E1_CRM_Entity"/>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity">
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E1_CRM_Entity"/>
        <rdfs:comment xml:lang="en">This class has as instances all entities in the ARIADNE infrastructure.</rdfs:comment>
        <rdfs:label xml:lang="en">AO Entity</rdfs:label>
    </rdfs:Class>   
    
    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Resource -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Resource">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity"/>
        <rdfs:comment xml:lang="en">AO_Resource has as instances all archaeological resources that are represented in the ARIADNE Content Cloud</rdfs:comment>
        <rdfs:label xml:lang="en">ARIADNE Resource</rdfs:label>
    </rdfs:Class>
	
    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Data_Resource -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Data_Resource">
        <rdfs:subClassOf rdf:resource="http://parthenos.d4science.org/CRMext/CRMpe/PE22_Persistent_Dataset"/>
		<rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E33_Linguistic_Object"/>
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Resource"/>
        <rdfs:comment xml:lang="en">This class is intended to describe the digital resources of the ARIADNE Research Infrastructure that are data. The type of an instance of this class (e.g., GIS, database, collection and the like) is specified through the has_type property. This is an abstract class: its instances are all and only the instances of its subclasses.</rdfs:comment>
        <rdfs:label xml:lang="en">AO Data Resource</rdfs:label>
    </rdfs:Class>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Individual_Data_Resource -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Individual_Data_Resource">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Data_Resource"/>
        <rdfs:comment xml:lang="en">This class has as instances data resources that from the ARIADNE point of view are atomic, that is no further decomposed. The complementary class is that of Collection, having as instances data resources that are composed of other data resources</rdfs:comment>
        <rdfs:label xml:lang="en">AO Individual Data Resource</rdfs:label>
    </rdfs:Class>
    
    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Document -->
    
    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Document">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Individual_Data_Resource"/>
        <rdfs:comment xml:lang="en">This class has as instances documents of interest to the ARIADNE Research Infrastructure</rdfs:comment>
        <rdfs:label xml:lang="en">AO Document</rdfs:label>
    </rdfs:Class>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Collection -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Collection">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Data_Resource"/>
        <rdfs:comment xml:lang="en">This class has as instances collections in the archaeological and archaeological-related domains. An archaeological collection is an aggregation of resources, called the members in the collection, instances of DataResource. As a consequence, collections can have other collections as members.</rdfs:comment>
		<rdfs:label xml:lang="en">AO Collection</rdfs:label>
	</rdfs:Class>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Service -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Service">
        <rdfs:subClassOf rdf:resource="http://parthenos.d4science.org/CRMext/CRMpe/PE8_E-Service"/>
		<rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E7_Activity"/>
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Resource"/>
        <rdfs:comment xml:lang="en">This class is intended to describe services in the archaeological domain. A service is “an offer by some actor of their willingness and ability to execute an activity or series of activities upon request" (PEM Specifications 3.1). Types of services are: stand-alone service, web service, front-office services and back-office service, as specified by the has_type property.</rdfs:comment>
        <rdfs:label xml:lang="en">AO Service</rdfs:label>
    </rdfs:Class>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Object -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Object">
		<rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E18_Physical_Thing"/>
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity"/>
        <rdfs:comment xml:lang="en">This class includes as instances physical objects of interest to the ARIADNE infrastructure.</rdfs:comment>
        <rdfs:label xml:lang="en">AO Object</rdfs:label>
    </rdfs:Class>


	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Event -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Event">
		<rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E5_Event"/>
        <rdfs:comment xml:lang="en">This class has as instances events of interest to ARIADNE, that is, events that play a role in the ARIADNE Content Cloud. An event is a “change of states in cultural, social or physical systems, regardless of scale, brought about by a series or group of coherent physical, cultural, technological or legal phenomena" (CIDOC CRM Specs 6.2).</rdfs:comment>
        <rdfs:label xml:lang="en">AO Event</rdfs:label>
    </rdfs:Class>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Activity -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Activity">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Event"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E7_Activity"/>
        <rdfs:comment xml:lang="en">This class includes as instances activities of interest to ARIADNE, that is, events that play a role in the ARIADNE Content Cloud. Activities are “actions intentionally carried out by actors that result in changes of state in the cultural, social, or physical systems documented" (CIDOC CRM Specs 6.2).</rdfs:comment>
        <rdfs:label xml:lang="en">AO Activity</rdfs:label>
    </rdfs:Class>

	
    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Agent -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Agent">
		<rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E39_Actor"/>
        <rdfs:comment xml:lang="en">“This class comprises people, either individually or in groups, who have the potential to perform intentional actions of kinds for which someone may be held responsible." (CIDOC CRM Specs 6.2)</rdfs:comment>
        <rdfs:label xml:lang="en">AO Agent</rdfs:label>
    </rdfs:Class>

    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Person -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Person">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Agent"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E21_Person"/>
        <rdfs:comment xml:lang="en">This class comprises individual human beings.</rdfs:comment>
        <rdfs:label xml:lang="en">AO Person</rdfs:label>
    </rdfs:Class>
    
    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Group -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Group">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Agent"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E74_Group"/>
        <rdfs:comment xml:lang="en">This class comprises “gatherings or organizations that act collectively or in a similar way due to any form of unifying relationship” [CRM Spec 6.2].</rdfs:comment>
        <rdfs:label xml:lang="en">AO Group</rdfs:label>
    </rdfs:Class>
    
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Temporal_Region -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Temporal_Region">
		<rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E52_Time-Span"/>
        <rdfs:comment xml:lang="en">This class comprises temporal extents, having a beginning, an end and a non-zero duration</rdfs:comment>
        <rdfs:label xml:lang="en">AO Temporal Region</rdfs:label>
    </rdfs:Class>
  
    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region">
		<rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Entity"/>
        <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
        <rdfs:comment xml:lang="en">This class comprises spatial regions having one of the following forms: a point on the surface of the Earth identified by latitude and longitude; a polygon as represented by GIS systems; a rectangular region on the surface of the Earth identified by its four vertices; any region identified by a IRI in a standard gazetteer, such as Geonames for modern places, Pleiades for ancient places.</rdfs:comment>
        <rdfs:label xml:lang="en">AO Spatial Region</rdfs:label>
    </rdfs:Class>
	
	<!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_Point -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_Point">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region"/>
        <rdfs:comment xml:lang="en">This class comprises points on the surface of the Earth identified by latitude and longitude.</rdfs:comment>
        <rdfs:label xml:lang="en">AO Spatial Region Point</rdfs:label>
    </rdfs:Class>

    <!-- https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_BBox -->

    <rdfs:Class rdf:about="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region_BBox">
        <rdfs:subClassOf rdf:resource="https://www.ariadne-infrastructure.eu/resource/ao/cat/1.1/AO_Spatial_Region"/>
